    "lint": "eslint --ext js,ts,tsx .",
    "prepare": "husky install"
  },
  "engines": {
    "node": "14.16.1"
  },
  "dependencies": {
    "front-matter": "^4.0.2",
    "highlight.js": "^10.0.3",
    "marked": "^1.1.0",
    "next": "^9.5.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@types/highlight.js": "^9.12.4",
    "@types/marked": "^0.7.4",
    "@types/node": "14.14.43",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "babel-eslint": "^10.0.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gh-pages": "^2.0.1",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "lint-staged": {
    "*.{js,ts}": [
