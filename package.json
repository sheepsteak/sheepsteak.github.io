{
  "name": "sheepsteak.github.io",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build && next export && yarn build:feed",
    "build:feed": "ts-node --skip-project -O '{ \"esModuleInterop\": true }' ./make-feed.ts",
    "dev": "next dev",
    "deploy": "gh-pages -d public -b master -u \"Chris Shepherd <chris@chrisshepherd.me>\"",
    "lint": "eslint --ext js,ts,tsx ."
  },
  "engines": {
    "node": "12.18.3"
  },
  "dependencies": {
    "front-matter": "^4.0.2",
    "highlight.js": "^10.0.3",
    "marked": "^1.1.0",
    "next": "^9.5.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@types/highlight.js": "^9.12.4",
    "@types/marked": "^0.7.4",
    "@types/node": "12",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2",
    "@typescript-eslint/parser": "^2",
    "babel-eslint": "^10",
    "eslint": "^6",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "4.0.0",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-react": "^7",
    "eslint-plugin-react-hooks": "^2",
    "gh-pages": "^2.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^9.2.1",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  }
}
