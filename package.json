  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "next build && next export && yarn build:feed",
    "build:feed": "ts-node --skip-project -O '{ \"esModuleInterop\": true }' ./make-feed.ts",
